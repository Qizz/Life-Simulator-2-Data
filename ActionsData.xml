    <commands>
      <player type="change" state="money" value="-8000000 - item.business_security_company.level * item.business_security_company.level * 7300000" />
      <item type="levelup" id="business_security_company" />
    </commands>
  </action>


  <action id="hire_security_company_guard" category="job">
    <requirements>
      <item id="business_security_company" operation="exist" />
