  <action id="create_mad_invention" category="job">
    <requirements>
      <job id="job_scientist" operation="exist" />
      <requirement exp="job.job_scientist.level gequal 6" />
      <requirement exp="item.item_mad_invention_part.count equal 25"/>
      <requirement exp="item.item_mad_invention.level != 2"/>
    </requirements>
    <commands>
      <player type="change" state="energy" value="-player.max_energy / 90" />
      <player type="change" state="mood" value="-player.max_mood / 80" />
      <player type="change" state="hunger" value="-player.max_hunger / 100 " />
      <item id="item_mad_invention" type="add" requirement="false"/>
      <item type="change" id="item_mad_invention" state="experience" value="Rand(1, 15)" requirement="false"/>
    </commands>
  </action>

  <action id="send_picture_exhibition" category="job">
    <requirements>
      <item id="item_picture" operation="exist"/>
      <item id="item_picture_in_exhibition" operation="unexist"/>
      <requirement exp="item.item_picture.level equal 2"/>
    </requirements>
    <commands>
      <player type="change" state="money" value="-700 * activity.activity_drawing_skills.level * activity.activity_drawing_skills.level" />
      <item type="remove" id="item_picture" value="1"/>
      <item type="add" id="item_picture_in_exhibition" value="1"/>
    </commands>
  </action>


  <action id="buy_restaurant" category="job">
    <requirements>
      <item id="business_restaurant" operation="unexist" />
      <requirement exp="job.job_cook.level gequal 5" />
    </requirements>
    <commands>
      <player type="change" state="money" value="-1500000" />
      <item type="add" id="business_restaurant"/>
    </commands>
  </action>

  <action id="upgrade_restaurant" category="job">
    <requirements>
      <item id="business_restaurant" operation="exist" />
      <item id="business_restaurant" operation="can_update" />
      <requirement exp="item.business_restaurant.restaurant_workers gequal item.business_restaurant.restaurant_workers_max"/>
    </requirements>
    <commands>
      <player type="change" state="money" value="-6400000 - item.business_restaurant.level * item.business_restaurant.level * 3500000" />
      <item type="levelup" id="business_restaurant" />
    </commands>
  </action>
  
  <action id="increase_restaurant_worker_salary" category="job">
    <requirements>
      <item id="business_restaurant" operation="exist" />
    </requirements>
    <commands>
      <item id="business_restaurant" state="restaurant_workers_salary" type="change" value="1"/>
    </commands>
  </action>
  
  <action id="decrease_restaurant_worker_salary" category="job">
    <requirements>
      <item id="business_restaurant" operation="exist" />
      <requirement exp="item.business_restaurant.restaurant_workers_salary greater 1" />
    </requirements>
    <commands>
      <item id="business_restaurant" state="restaurant_workers_salary" type="change" value="-1"/>
    </commands>
  </action>

  <action id="buy_business_security_company" category="job">
    <requirements>
      <item id="business_security_company" operation="unexist" />
      <requirement exp="job.job_police.level gequal 5" />
