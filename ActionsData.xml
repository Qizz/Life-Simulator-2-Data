      <player type="change" state="mood" value="-0.7 * job.job_cook.level - player.max_mood / 1200 * job.job_cook.level" />
      <job id="job_cook" type="change" state="experience" value="job.job_cook.level * 0.11 + 1" />
      <activity id="activity_cooking_skills" state="experience" value="0.1  + activity.activity_cooking_skills.level * 0.4"/>
    </commands>
  </action>
  
  <action id="begin_career_scientist" category="job">
