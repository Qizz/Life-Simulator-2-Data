  </action>

  <action id="do_cook_job" category="job">
    <requirements>
      <job id="job_cook" operation="exist" />
      <job id="job_cook" operation="not_maxlevel" />
    </requirements>
    <commands>
      <player type="change" state="energy" value="-2 * job.job_cook.level - player.max_energy / 1000 * job.job_cook.level" />
