<ActionsData>
  <!--================================================
GENERAL
================================================-->
  
  <action id="earn_some_money" category="general">
    <commands>
      <player type="change" state="money" value="1 + item.clicker_helper.count + Pow(item.clicker_helper.count, 2) / 60" />
      <player type="change" state="energy" value="-1.5 - item.clicker_helper.count * item.clicker_helper.count / 18" />
      <player type="change" state="mood" value="-0.8 - item.clicker_helper.count * item.clicker_helper.count / 12" />
      <player type="change" state="hunger" value="-0.5 - item.clicker_helper.count * item.clicker_helper.count / 16" />
      <event id="event_found_coins"/>
    </commands>
  </action>

  <action id="sleep_bed" category="general">
    <requirements>
      <item id="item_bed" operation="exist"/>
    </requirements>
    <commands>
      <player type="change" state="energy" value="13 * item.item_bed.level + player.max_energy / 23 * item.item_bed.level" />
      <player type="change" state="hunger" value="-5 * item.item_bed.level - player.max_hunger / 700 * item.item_bed.level" />
      <player type="change" state="mood" value="10 * item.item_bed.level + player.max_mood / 40 * item.item_bed.level" />
    </commands>
  </action>
  
  <action id="watch_tv" category="general">
    <requirements>
      <item id="item_tv" operation="exist" value="1" />
    </requirements>
    <commands>
      <player type="change" state="energy" value="-2.5 * item.item_tv.level - player.max_energy / 44 * item.item_tv.level" />
      <player type="change" state="hunger" value="-5 * item.item_tv.level - player.max_hunger / 35 * item.item_tv.level" />
      <player type="change" state="mood" value="15 * item.item_tv.level + player.max_mood / 19 * item.item_tv.level" />
    </commands>
  </action>

  <action id="play_computer_games" category="general">
    <requirements>
      <item id="item_computer" operation="exist" value="1" />
    </requirements>
    <commands>
      <player type="change" state="energy" value="-2 * item.item_computer.level - player.max_energy / 45 * item.item_computer.level" />
      <player type="change" state="hunger" value="-2 * item.item_computer.level - player.max_hunger / 40 *  item.item_computer.level" />
      <player type="change" state="mood" value="10 * item.item_computer.level + player.max_mood / 20 * item.item_computer.level" />
    </commands>
  </action>
  
  <action id="cook_food" category="general">
    <requirements>
      <item id="item_stove" operation="exist" />
    </requirements>
    <commands>
      <item type="remove" id="item_foodstuff" value="item.item_stove.level * 2.5 - item.item_fridge.level * 1.5"></item>
      <player type="change" state="energy" value="-3 - item.item_stove.level * 2 - player.max_energy / 65 * item.item_stove.level " />
      <player type="change" state="mood" value="-1 - item.item_stove.level * 0.2 - player.max_mood / 120 * item.item_stove.level " />
      <player type="change" state="hunger" value="player.max_hunger / 30 * item.item_stove.level + item.item_fridge.level * 20 + player.max_hunger / 20 * activity.activity_cooking_skills.level" />

      <activity type="add" id="activity_cooking_skills" requirement="false"/>
      <activity id="activity_cooking_skills" state="experience" value="0.3 + activity.activity_cooking_skills.level * 0.1 + item.item_stove.level * 0.3" requirement="false"/>
    </commands>
  </action>

  <action id="sleep_hotel" category="general">
    <requirements>
      <item id="item_apartment" operation="unexist"/>
    </requirements>
    <commands>
      <player type="change" state="energy" value="40" />
      <player type="change" state="hunger" value="-10" />
      <player type="change" state="mood" value="10" />
      <player type="change" state="money" value="-10 + player.money / -600" />
    </commands>
  </action>

  <action id="eat_fastfood" category="general">
    <requirements>
      <item id="item_stove" operation="unexist" />
    </requirements>
    <commands>
      <player type="change" state="money" value="-3 + player.money / -500" />
      <player type="change" state="energy" value="5" />
      <player type="change" state="hunger" value="50" />
    </commands>
  </action>

  <action id="go_to_the_cinema" category="general">
    <requirements>
      <item id="item_tv" operation="unexist" />
    </requirements>
    <commands>
      <player type="change" state="money" value="-4 + player.money / -400" />
      <player type="change" state="energy" value="-5" />
      <player type="change" state="mood" value="60" />
    </commands>
  </action>

  <action id="eat_restaurant" category="general">
    <requirements>
      <item id="item_stove" operation="exist" />
      <requirement exp="player.money less 1000000"/>
    </requirements>
    <commands>
      <player type="change" state="money" value="-50 + player.money / -80" />
      <player type="change" state="energy" value="player.max_energy / 8" />
      <player type="change" state="mood" value="player.max_mood / 10" />
      <player type="change" state="hunger" value="player.max_hunger / 1.8" />
    </commands>
  </action>

  <action id="drink_elixir_of_youth" category="general">
    <requirements>
      <item id="item_elixir_of_youth" operation="exist" />
    </requirements>
    <commands>
      <impact id="impact_elixir_of_youth" type="add"/>
      <item id="item_elixir_of_youth" type="remove"/>
    </commands>
  </action>
  
  <action id="drink_elixir_of_regeneration" category="general">
    <requirements>
      <item id="item_elixir_of_regeneration" operation="exist" />
    </requirements>
    <commands>
      <player type="change" state="energy" value="player.max_energy * 10" />
      <player type="change" state="mood" value="player.max_mood * 10" />
      <player type="change" state="hunger" value="player.max_hunger * 10" />
      <item id="item_elixir_of_regeneration" type="remove"/>
    </commands>
  </action>
  
  <action id="go_to_doctor" category="general">
    <requirements>
      <requirement exp="impact.cruel_disease.duration_left greater 0"/>
    </requirements>
    <commands>
      <impact id="cruel_disease" type="remove"/>
      <player type="change" state="money" value="-300 + player.money / -250" />
    </commands>
  </action>
  
  <action id="take_medicine" category="general">
    <requirements>
      <requirement exp="impact.mild_disease.duration_left greater 0"/>
    </requirements>
    <commands>
      <impact id="mild_disease" type="remove"/>
      <player type="change" state="money" value="-10 + player.money / -800" />
    </commands>
  </action>
  
