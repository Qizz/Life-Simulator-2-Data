  <action id="upgrade_business_security_company" category="job">
    <requirements>
      <item id="business_security_company" operation="exist" />
      <item id="business_security_company" operation="can_update" />
      <requirement exp="item.business_security_company.business_security_company_guards gequal item.business_security_company.business_security_company_guards_max"/>
      <requirement exp="item.business_security_company.business_security_company_prevented_crimes_total gequal 10 * item.business_security_company.level * item.business_security_company.level"/>
