JOB
================================================-->

  <action id="begin_career_programmer" category="job">
    <requirements>
      <job operation="no_job" />
      <requirement exp="player.day greater 20"/>
      <requirement exp="activity.activity_programming_skills.level greater 1"/>
      <item id="item_computer" operation="exist"/>
    </requirements>
    <commands>
      <job id="job_programmer" type="add" />
    </commands>
  </action>

  <action id="do_programmer_job" category="job">
    <requirements>
      <job id="job_programmer" operation="exist" />
    </requirements>
    <commands>
      <player type="change" state="energy" value="-2 * job.job_programmer.level - player.max_energy / 1000 * job.job_programmer.level" />
      <player type="change" state="hunger" value="-1.3 * job.job_programmer.level - player.max_hunger / 1600 * job.job_programmer.level" />
      <player type="change" state="mood" value="-0.5 * job.job_programmer.level- player.max_mood / 1600 * job.job_programmer.level" />
      <job id="job_programmer" type="change" state="experience" value="job.job_programmer.level * 0.08 + 1" />
      <activity id="activity_programming_skills" state="experience" value="0.4 + activity.activity_programming_skills.level * 0.12"/>
      <item id="item_program" type="add" requirement="false"/>
      <item type="change" id="item_program" state="experience" value="0.2" requirement="false"/>
    </commands>
  </action>
  
  <action id="begin_career_artist" category="job">
    <requirements>
      <job operation="no_job" />
      <requirement exp="player.day greater 20"/>
      <requirement exp="activity.activity_drawing_skills.level gequal 2"/>
    </requirements>
    <commands>
      <job id="job_artist" type="add" />
    </commands>
  </action>

  <action id="do_artist_job" category="job">
    <requirements>
      <job id="job_artist" operation="exist" />
    </requirements>
    <commands>
      <player type="change" state="energy" value="-2 * job.job_artist.level - player.max_energy / 1000 * job.job_artist.level" />
      <player type="change" state="hunger" value="-1.2 * job.job_artist.level - player.max_hunger / 1600 * job.job_artist.level" />
      <player type="change" state="mood" value="-0.4 * job.job_artist.level - player.max_mood / 1600 * job.job_artist.level" />
      <job id="job_artist" type="change" state="experience" value="job.job_artist.level * 0.09 + 1" />
      <activity id="activity_drawing_skills" state="experience" value="0.4  + activity.activity_drawing_skills.level * 0.13"/>
      <item id="item_picture" type="add" requirement="false"/>
      <item type="change" id="item_picture" state="experience" value="0.4" requirement="false"/>
    </commands>
  </action>
  
  
  <action id="begin_career_cook" category="job">
